all: \
    trustnote-deposit.o \
    deposit_test.o \
    deposit_test


trustnote-deposit.o:
	g++ -std=c++11 -fPIC -c -g -D _DEBUG -I. -I../../include/ ../../include/trustnote-deposit.cpp

deposit_test.o:
	g++ -std=c++11 -fPIC -c -g -D _DEBUG -I. -I../../include/ ./deposit_test.cpp

deposit_test:
	g++ -fPIC -g \
	    -o deposit_test \
	    ./deposit_test.o ./trustnote-deposit.o



.PHONY: clean
clean:
	rm test miner *.o *.so *.a miner.*.debug miner.*.release
